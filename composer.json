{
    "name":         "kagency/kagent",
    "description":  "Information managing agent",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Kagency/module-rss.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Kagency": "src/"
        }
    },
    "require": {
        "silex/silex": "@stable",
        "symfony/config": "@stable",
        "symfony/dependency-injection": "@stable",
        "kagency/module-rss": "@dev"
    },
    "require-dev": {
        "behat/behat": "@stable",
        "pdepend/pdepend": "@stable",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "@stable",
        "sebastian/phpcpd": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    }
}
